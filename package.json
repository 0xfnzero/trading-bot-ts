{
  "name": "trading-proxy-client",
  "version": "1.0.0",
  "description": "TypeScript client for trading-proxy-http",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/bot-runner.js",
    "bot": "ts-node src/bot-runner.ts",
    "bot:prod": "npm run build && node dist/bot-runner.js",
    "generate-config": "ts-node src/bot-runner.ts --generate-config",
    "subscribe": "ts-node src/simple-subscriber.ts",
    "subscribe:prod": "npm run build && node dist/simple-subscriber.js"
  },
  "keywords": ["solana", "trading", "dex"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.18.0",
    "@solana/web3.js": "^1.95.8",
    "bs58": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.13",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.2"
  }
}